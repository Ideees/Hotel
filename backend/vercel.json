{
    "version":2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/"
        }
    ]
  },
{
  "headers": [
    {
      "source": "/Huser",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://hotel-front-taupe.vercel.app"
        }
      ]
    }
  ]
}

